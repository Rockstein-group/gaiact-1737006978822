import imgimages_gaiact_leaft_icon_webp from "../../public/images/gaiact_leaft_icon.webp";
import imgimages_gaiact_business_meetup_img_webp from "../../public/images/gaiact_business_meetup_img.webp";
import imgimages_gaiact_mission_cloud_deco_webp from "../../public/images/gaiact_mission_cloud_deco.webp";
import imgimages_gaiact_mission_cloud_deco_bottom_webp from "../../public/images/gaiact_mission_cloud_deco_bottom.webp";
import imgimages_gaiact_windmill_icon_webp from "../../public/images/gaiact_windmill_icon.webp";
import imgimages_gaiact_event_summary_blog_webp from "../../public/images/gaiact_event_summary_blog.webp";
import imgimages_chevron_right_dark_webp from "../../public/images/chevron-right-dark.webp";
import imgimages_gaiact_instagram_icon_svg from "../../public/images/gaiact_instagram_icon.svg";
import imgimages_gaiact_linkedin_icon_svg from "../../public/images/gaiact_linkedin_icon.svg";
import imgimages_gaiact_send_icon_webp from "../../public/images/gaiact_send_icon.webp";
